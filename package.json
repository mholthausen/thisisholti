{
  "name": "thisisholti",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "start:dev": "vite --config vite.config.js serve",
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && vite --config vite.config.js build",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "test": "npm run lint && jest --coverage --passWithNoTests",
    "preview": "vite --config vite.config.js preview"
  },
  "keywords": [],
  "author": "Michael Holthausen <holthausen@terrestris.de> (https://www.terrestris.de/)",
  "license": "Apache-2.0",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "build"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(scss|css)$": "jest-transform-stub"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.23.2",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@terrestris/eslint-config-typescript": "4.0.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@types/react": "18.2.36",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vitejs/plugin-react": "4.1.1",
    "babel-jest": "29.7.0",
    "eslint": "8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "rimraf": "5.0.5",
    "sass": "1.69.5",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.5.0"
  },
  "dependencies": {
    "@csstools/normalize.css": "12.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0"
  }
